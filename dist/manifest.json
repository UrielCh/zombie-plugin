{
  "background": {
    "page": "files/background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "files/icon.png",
    "default_popup": "files/popup.html",
    "default_title": "zombie Help"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": ["/vendor/jquery.3.min.js", "/js/client.js"],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Dynamicaly improve Your chrome navigator",
  "devtools_page": "devtools.html",
  "externally_connectable": {
    "matches": [
      "*://127.0.0.1/*",
      "*://localhost/*",
      "*://*.exs.fr/*",
      "*://*.xulu.fr/*",
      "*://*.google.com/*",
      "*://*.google.fr/*"
    ]
  },
  "icons": {
    "64": "files/icon.png"
  },
  "incognito": "spanning",
  "manifest_version": 2,
  "minimum_chrome_version": "32",
  "name": "Zombie",
  "offline_enabled": true,
  "omnibox": {
    "keyword": "zombie"
  },
  "key": "",
  "permissions": [
    "browsingData",
    "tabs",
    "storage",
    "unlimitedStorage",
    "cookies",
    "webRequest",
    "webRequestBlocking",
    "geolocation",
    "webNavigation", 
    "proxy",
    "desktopCapture",
    "<all_urls>"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "4.0.4",
  "web_accessible_resources": [
    "/js/background.js",
    "/js/zFunction.js",
    "/js/client.js",
    "http://*/*",
    "https://*/*"
  ]
}
