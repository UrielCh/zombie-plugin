{
  "name": "plugin",
  "version": "4.0.11",
  "description": "",
  "main": ".eslintrc.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf built",
    "tsc": "tsc -p .",
    "tsc:watch": "tsc --watch -p .",
    "browserifyBG:watch": "watchify ./built/background.js -o ./dist/js/background.js",
    "browserifyPOP:watch": "watchify ./built/popup.js     -o ./dist/js/popup.js",
    "browserifyCLI:watch": "cpx --watch ./built/client.js    ./dist/js/",
    "watch": "concurrently npm:*:watch",
    "lint": "tslint -p .",
    "browserify-BG": "browserify  ./built/background.js -o ./dist/js/background.js",
    "browserify-POP": "browserify ./built/popup.js      -o ./dist/js/popup.js",
    "browserify-CLI": "cpx        ./built/client.js        ./dist/js/",
    "browserify": "concurrently npm:browserify-*",
    "build": "npm run lint && npm run tsc && npm run browserify"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "base64-js": "^1.3.1",
    "bootstrap4-toggle": "^3.6.1",
    "chrome-promise": "^3.0.5",
    "jsqr": "^1.2.0",
    "typescript": "^3.7.3"
  },
  "devDependencies": {
    "@types/base64-js": "^1.2.5",
    "@types/bluebird": "^3.5.29",
    "@types/chrome": "0.0.91",
    "@types/jquery": "^3.3.31",
    "@types/puppeteer": "^2.0.0",
    "bluebird": "^3.7.2",
    "concurrently": "^5.0.0",
    "cpx": "^1.5.0",
    "puppeteer": "^2.0.0",
    "rimraf": "^3.0.0",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "watchify": "^3.11.1"
  }
}
