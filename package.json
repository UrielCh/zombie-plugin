{
  "name": "plugin",
  "version": "4.1.15",
  "description": "",
  "main": ".eslintrc.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf built",
    "tsc": "tsc -p .",
    "tsc:watch": "tsc --watch -p .",
    "browserifyBG:watch": "watchify  ./built/background.js -o ./dist/js/background.js",
    "browserifyPOP:watch": "watchify ./built/popup.js     -o ./dist/js/popup.js",
    "browserifyCLI:watch": "watchify ./built/client.js    -o ./dist/js/client.js",
    "watch": "concurrently npm:*:watch",
    "lint": "eslint -c .eslintrc.js  ./src/*.ts",
    "browserify-BG": "browserify  ./built/background.js -o ./dist/js/background.js",
    "browserify-POP": "browserify ./built/popup.js      -o ./dist/js/popup.js",
    "browserify-CLI": "browserify ./built/client.js     -o ./dist/js/client.js",
    "browserify": "concurrently npm:browserify-*",
    "build": "npm run tsc && npm run browserify",
    "install": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "base64-js": "^1.3.1",
    "bootstrap4-toggle": "^3.6.1",
    "chrome-promise": "^3.0.5",
    "jsqr": "^1.3.1",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@types/base64-js": "^1.3.0",
    "@types/bluebird": "^3.5.32",
    "@types/chai": "^4.2.12",
    "@types/chrome": "0.0.123",
    "@types/fs-extra": "^9.0.1",
    "@types/jquery": "^3.5.1",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.1",
    "@types/puppeteer": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.1.1",
    "bluebird": "^3.7.2",
    "chai": "^4.2.0",
    "concurrently": "^5.3.0",
    "cpx": "^1.5.0",
    "eslint": "^7.13.0",
    "fs-extra": "^9.0.1",
    "mocha": "^8.1.3",
    "puppeteer": "^5.3.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "watchify": "^3.11.1"
  }
}
